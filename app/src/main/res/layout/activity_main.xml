<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/teks2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="18dp"
        android:text="TM1_22090110_LAELA DIYAH PUSPITA_2B"
        android:textSize="18dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="5dp"
        tools:ignore="MissingConstraints"/>

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Submit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teks2"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/teks3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:text="MESSAGE"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/teks2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="7dp"/>

    <TextView
        android:id="@+id/teks4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="168dp"
        android:text="STATUS"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/teks2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"/>

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="293dp"
        android:layout_height="52dp"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Data Received"
        app:layout_constraintTop_toBottomOf="@id/teks3"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="7dp" />

    <EditText
        android:id="@+id/editTextTextPersonName1"
        android:layout_width="293dp"
        android:layout_height="52dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Halo Siswa Algo2"
        app:layout_constraintTop_toBottomOf="@id/teks4"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="7dp"/>

    <TextView
        android:id="@+id/teks5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="276dp"
        android:text="COMMENT"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/teks2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="14dp"/>

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="293dp"
        android:layout_height="52dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Anda Sudah Terkoneksi ke Server Mimoapps"
        app:layout_constraintTop_toBottomOf="@id/teks5"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="7dp"/>

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CLOSE"
        tools:layout_editor_absoluteX="284dp"
        tools:layout_editor_absoluteY="448dp"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toBottomOf="@id/editTextTextPersonName2"/>

</androidx.constraintlayout.widget.ConstraintLayout>